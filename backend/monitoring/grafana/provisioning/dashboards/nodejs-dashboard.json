{
  "id": null,
  "title": "Node.js Metrics",
  "tags": ["nodejs", "monitoring", "errors"],
  "timezone": "browser",
  "schemaVersion": 36,
  "version": 2,
  "panels": [
    {
      "type": "graph",
      "title": "CPU Usage (seconds)",
      "targets": [
        {
          "expr": "process_cpu_seconds_total",
          "legendFormat": "{{instance}}",
          "refId": "A"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "Memory Usage (bytes)",
      "targets": [
        {
          "expr": "process_resident_memory_bytes",
          "legendFormat": "{{instance}}",
          "refId": "B"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "HTTP Request Rate (/health)",
      "targets": [
        {
          "expr": "rate(http_requests_total{path=\"/health\",status=\"200\"}[5m])",
          "legendFormat": "200 OK",
          "refId": "C"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "Request Duration (95th percentile)",
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum(rate(http_request_duration_seconds_bucket[5m])) by (le))",
          "legendFormat": "95th percentile",
          "refId": "D"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "HTTP 4xx Errors",
      "targets": [
        {
          "expr": "sum by (status) (rate(http_requests_total{status=~\"4..\"}[5m]))",
          "legendFormat": "Status {{status}}",
          "refId": "E"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "HTTP 5xx Errors",
      "targets": [
        {
          "expr": "sum by (status) (rate(http_requests_total{status=~\"5..\"}[5m]))",
          "legendFormat": "Status {{status}}",
          "refId": "F"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "Login Errors (401)",
      "targets": [
        {
          "expr": "rate(http_requests_total{status=\"401\",path=\"/api/auth/login\"}[5m])",
          "legendFormat": "401 Errors",
          "refId": "G"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "Rate Limiter Triggered (429)",
      "targets": [
        {
          "expr": "rate(http_requests_total{status=\"429\",path=\"/api/auth/login\"}[5m])",
          "legendFormat": "429 Rate Limits",
          "refId": "H"
        }
      ],
      "datasource": "Prometheus"
    },
    {
      "type": "graph",
      "title": "All Requests Count",
      "targets": [
        {
          "expr": "sum(rate(http_requests_total[5m]))",
          "legendFormat": "All Requests",
          "refId": "I"
        }
      ],
      "datasource": "Prometheus"
    }
  ]
}
